<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fun.crawl.mapper.ManagerMapper">

<!-- 通用查询映射结果 -->
<resultMap id="BaseResultMap" type="com.fun.crawl.model.Manager">
    <id column="id" property="id"/>
        <result column="username" property="username"/>
      <result column="password" property="password"/>
      <result column="create_time" property="createTime"/>
      <result column="update_time" property="updateTime"/>
      <result column="lastlogin_time" property="lastloginTime"/>
      <result column="prelogin_time" property="preloginTime"/>
      <result column="login_ip" property="loginIp"/>
      <result column="prelogin_ip" property="preloginIp"/>
      <result column="enable" property="enable"/>
      <result column="remarks" property="remarks"/>
  </resultMap>

<!-- 通用查询结果列 -->
<sql id="Base_Column_List">
       id, username, password, create_time, update_time, lastlogin_time, prelogin_time, login_ip, prelogin_ip, enable, remarks
</sql>
<select id="pageByQuery" resultMap="BaseResultMap">
    select     id, username, password, create_time, update_time, lastlogin_time, prelogin_time, login_ip, prelogin_ip, enable, remarks
    from t_manager
    <where>
        <if test="id !=null and id!=''">
           and id = #{ id}
        </if>
        <if test="username !=null and username!=''">
           and username = #{ username}
        </if>
        <if test="password !=null and password!=''">
           and password = #{ password}
        </if>
        <if test="createTime !=null and createTime!=''">
           and create_time = #{ createTime}
        </if>
        <if test="updateTime !=null and updateTime!=''">
           and update_time = #{ updateTime}
        </if>
        <if test="lastloginTime !=null and lastloginTime!=''">
           and lastlogin_time = #{ lastloginTime}
        </if>
        <if test="preloginTime !=null and preloginTime!=''">
           and prelogin_time = #{ preloginTime}
        </if>
        <if test="loginIp !=null and loginIp!=''">
           and login_ip = #{ loginIp}
        </if>
        <if test="preloginIp !=null and preloginIp!=''">
           and prelogin_ip = #{ preloginIp}
        </if>
        <if test="enable !=null and enable!=''">
           and enable = #{ enable}
        </if>
        <if test="remarks !=null and remarks!=''">
           and remarks = #{ remarks}
        </if>
    </where>
</select>


</mapper>
